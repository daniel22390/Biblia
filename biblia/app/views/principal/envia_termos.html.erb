  $('body').on('click', '.btn_up', function(ev){
    if( $(this).parent().parent().parent().attr("position") === "2"){
      $(this).parent().parent().parent().find('.btn_up').hide();
      $(this).parent().parent().parent().prev().find('.btn_up').show();
    }
    if( $(this).parent().parent().parent().attr("position") === "10"){
      $(this).parent().parent().parent().find('.btn_down').show();
      $(this).parent().parent().parent().prev().find('.btn_down').hide();
    }

    $(this).parent().parent().parent().attr("position",  parseInt($(this).parent().parent().parent().attr('position')) - 1);
    $(this).parent().parent().parent().prev().attr("position",  parseInt($(this).parent().parent().parent().attr('position')) + 1);
    $(this).parent().parent().parent().after($(this).parent().parent().parent().prev());
  });

  $('body').on('click', '.btn_down', function(ev){
    if( $(this).parent().parent().parent().attr("position") === "9"){
      $(this).parent().parent().parent().find('.btn_down').hide();
      $(this).parent().parent().parent().next().find('.btn_down').show();
    }
    if( $(this).parent().parent().parent().attr("position") === "1"){
      $(this).parent().parent().parent().find('.btn_up').show();
      $(this).parent().parent().parent().next().find('.btn_up').hide();
    }

    $(this).parent().parent().parent().attr("position",  parseInt($(this).parent().parent().parent().attr('position')) + 1);
    $(this).parent().parent().parent().next().attr("position",  parseInt($(this).parent().parent().parent().attr('position')) - 1);
    $(this).parent().parent().parent().before($(this).parent().parent().parent().next());
  });

